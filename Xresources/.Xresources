! X colors.
*foreground:        #e0e0e0
*background:        #2C3E50
*.foreground:       #e0e0e0
*.background:       #2C3E50
emacs*foreground:   #e0e0e0
emacs*background:   #2C3E50
URxvt*foreground:   #e0e0e0
XTerm*foreground:   #e0e0e0
UXTerm*foreground:  #e0e0e0
URxvt*background:   [100]#2C3E50
XTerm*background:   #2C3E50
UXTerm*background:  #2C3E50
URxvt*cursorColor:  #e0e0e0
XTerm*cursorColor:  #e0e0e0
UXTerm*cursorColor: #e0e0e0
URxvt*borderColor:  [100]#2C3E50

! Colors 0-15.
*.color0: #2C3E50
*color0:  #2C3E50
*.color1: #E74C3C
*color1:  #E74C3C
*.color2: #2ECC71
*color2:  #2ECC71
*.color3: #F1C40F
*color3:  #F1C40F
*.color4: #3498DB
*color4:  #3498DB
*.color5: #9B59B6
*color5:  #9B59B6
*.color6: #1ABC9C
*color6:  #1ABC9C
*.color7: #e0e0e0
*color7:  #e0e0e0
*.color8: #95A5A6
*color8:  #95A5A6
*.color9: #E74C3C
*color9:  #E74C3C
*.color10: #2ECC71
*color10:  #2ECC71
*.color11: #F1C40F
*color11:  #F1C40F
*.color12: #3498DB
*color12:  #3498DB
*.color13: #9B59B6
*color13:  #9B59B6
*.color14: #1ABC9C
*color14:  #1ABC9C
*.color15: #ECF0F1
*color15:  #ECF0F1

! Black color that will not be affected by bold highlighting.
*.color66: #2C3E50
*color66:  #2C3E50

! Xclock colors.
XClock*foreground: #e0e0e0
XClock*background: #2C3E50
XClock*majorColor:  rgba:ec/f0/f1/ff
XClock*minorColor:  rgba:ec/f0/f1/ff
XClock*hourColor:   rgba:ec/f0/f1/ff
XClock*minuteColor: rgba:ec/f0/f1/ff
XClock*secondColor: rgba:ec/f0/f1/ff

! Set depth to make transparency work.
URxvt*depth: 32

!! URxvt Appearance
URxvt.font: -*-canele-*
URxvt.boldFont: -*-canele-*
URxvt.italicFont: -*-canele-*
URxvt.boldItalicfont: -*-canele-*
URxvt.letterSpace: 0
URxvt.lineSpace: 0
!! URxvt.geometry: 74x16
URxvt.geometry: 100x25
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false

!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

!! Copy Paste & Other Extensions
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1

!! Font Stuff
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
