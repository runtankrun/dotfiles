! Black color that will not be affected by bold highlighting.
*.color66: #282c34
*color66:  #282c34

! Xclock colors.
XClock*foreground: #a9b1d6
XClock*background: #1a1b26
XClock*majorColor:  rgba:c0/ca/f5/ff
XClock*minorColor:  rgba:c0/ca/f5/ff
XClock*hourColor:   rgba:c0/ca/f5/ff
XClock*minuteColor: rgba:c0/ca/f5/ff
XClock*secondColor: rgba:c0/ca/f5/ff

! Set depth to make transparency work.
URxvt*depth: 32

