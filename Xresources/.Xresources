! Black color that will not be affected by bold highlighting.
*.color66: #0e0f12
*color66:  #0e0f12

! Xclock colors.
XClock*foreground: #93e4e4
XClock*background: #0e0f12
XClock*majorColor:  rgba:93/e4/e4/ff
XClock*minorColor:  rgba:93/e4/e4/ff
XClock*hourColor:   rgba:93/e4/e4/ff
XClock*minuteColor: rgba:93/e4/e4/ff
XClock*secondColor: rgba:93/e4/e4/ff

! Set depth to make transparency work.
URxvt*depth: 32
